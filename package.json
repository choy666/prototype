{
  "$schema": "https://json.schemastore.org/package",
  "name": "vercel-marketplace-neon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "db:backup": "node scripts/backup.js",
    "db:restore": "node scripts/restore.js",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "npx playwright test",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check:env": "ts-node scripts/check-env.ts",
    "validate:env": "ts-node -e \"require('./lib/utils/env-validation').validateEnvOnStartup()\"",
    "mp:webhook-secret": "node scripts/get-mp-webhook-secret.js",
    "dev:safe": "npm run validate:env && npm run dev",
    "build:safe": "npm run validate:env && npm run build",
    "verify:checkout": "node scripts/verify-checkout.js",
    "tunnel": "lt --port 3000 --subdomain prototypev3",
    "dev:tunnel": "concurrently \"npm run dev\" \"npm run tunnel\"",
    "mcp:mercadolibre": "node mcp/mercadolibre-server.js",
    "mcp:mercadopago": "node mcp/mercadopago-server.js",
    "ts-node": "ts-node",
    "migrate:ml-shipping": "ts-node scripts/migrate-ml-shipping.ts",
    "migrate:simple": "ts-node scripts/simple-migrate.ts",
    "audit:products": "ts-node scripts/migration-me2/audit/run-audit.ts",
    "migrate:products-dimensions": "ts-node scripts/migration-me2/migrate-products-dimensions.ts",
    "migrate:products-rollback": "ts-node scripts/migration-me2/migrate-products-dimensions.ts rollback"
  },
